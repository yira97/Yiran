{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RsaSettings": {
    "PemFilePath": {
      "PublicKeyFilePath": "<RSA_PUBLIC_KEY_FILE_PATH>",
      "PrivateKeyFilePath": "<RSA_PRIVATE_KEY_FILE_PATH>"
    }
  },
  "JwtSettings": {
    "Issuer": "<JWT_ISSUER>",
    "Audience": "<JWT_AUDIENCE>"
  },
  "AwsSettings": {
    "AccessKeyId": "<AWS_ACCESS_KEY_ID>",
    "SecretAccessKey": "<AWS_SECRET_ACCESS_KEY>",
    "Region": "<AWS_REGION>",
    "S3Settings": {
      "BucketName": "<AWS_S3_BUCKET_NAME>"
    },
    "AwsSolutionSettings": {
      "ServerlessImageHandlerSolutionConfiguration": {
        "SecretsManagerName": "<AWS_SOLUTION_SERVERLESS_IMAGE_HANDLER_SECRET_MANAGER_NAME>",
        "SecretKey": "<AWS_SOLUTION_SERVERLESS_IMAGE_HANDLER_SECRET_KEY>",
        "DistributionDomainName": "<AWS_SOLUTION_SERVERLESS_IMAGE_HANDLER_DISTRIBUTION_DOMAIN_NAME>"
      }
    }
  },
  "ConnectionSettings": {
    "Postgres": {
      "Address": "<POSTGRES_ADDRESS>",
      "Port": "<POSTGRES_PORT>",
      "Username": "<POSTGRES_USERNAME>",
      "Password": "<POSTGRES_PASSWORD>",
      "Database": "<POSTGRES_DATABASE>"
    }
  }
}
