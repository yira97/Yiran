{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "auth.rockmoss.evrane.com",
    "Audience": "rockmoss.evrane.com",
    "IssuerKeyPath": "<ROCKMOSS_AUTH_JWT_ISSUER_KEY>"
  },
  "AwsSettings": {
    "AccessKeyId": "<ROCKMOSS_ROCKMOSS_AWS_S3_ACCESS_KEY_ID>",
    "SecretAccessKey": "<ROCKMOSS_ROCKMOSS_AWS_S3_SECRET_ACCESS_KEY>",
    "Region": "ap-northeast-1",
    "S3": {
      "BucketName": "evrane-rockmoss"
    },
    "Solutions": {
      "ServerlessImageHandlerSolution": {
        "SecretsManagerName": "RockMoss-SecretForUrlSignature",
        "SecretKey": "SecretForUrlSignature",
        "DistributionDomainName": "dy9thd0ewjmx0.cloudfront.net"
      }
    }
  },
  "ConnectionSettings": {
    "Postgres": {
      "Address": "<ROCKMOSS_ROCKMOSS_POSTGRES_ADDRESS>",
      "Port": "<ROCKMOSS_ROCKMOSS_POSTGRES_PORT>",
      "Username": "<ROCKMOSS_ROCKMOSS_POSTGRES_USERNAME>",
      "Password": "<ROCKMOSS_ROCKMOSS_POSTGRES_PASSWORD>",
      "Database": "<ROCKMOSS_ROCKMOSS_POSTGRES_DATABASE>"
    },
    "Redis": {
      "Address": "<ROCKMOSS_ROCKMOSS_REDIS_ADDRESS",
      "Port": "<ROCKMOSS_ROCKMOSS_REDIS_PORT>",
      "Password": "<ROCKMOSS_ROCKMOSS_REDIS_PASSWORD>"
    },
    "RabbitMq": {
      "Address": "<ROCKMOSS_ROCKMOSS_RABBITMQ_ADDRESS>",
      "Port": "<ROCKMOSS_ROCKMOSS_RABBITMQ_PORT",
      "Username": "<ROCKMOSS_ROCKMOSS_RABBITMQ_USERNAME>",
      "Password": "<ROCKMOSS_ROCKMOSS_RABBITMQ_PASSWORD>"
    }
  }
}
