{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RsaSettings": {
    "PemFilePath": {
      "PublicKeyFilePath": "<RSA_PUBLIC_KEY_FILE_PATH>",
      "PrivateKeyFilePath": "<RSA_PRIVATE_KEY_FILE_PATH>"
    }
  },
  "JwtSettings": {
    "Issuer": "api.blog.evrane.com",
    "Audience": "blog.evrane.com",
    "IssuerKeyPath": "<JWT_ISSUER_KEY>"
  },
  "AwsSettings": {
    "AccessKeyId": "<AWS_S3_ACCESS_KEY_ID>",
    "SecretAccessKey": "<AWS_S3_SECRET_ACCESS_KEY>",
    "Region": "ap-northeast-1",
    "S3": {
      "BucketName": "evrane-rockmoss"
    },
    "Solutions": {
      "ServerlessImageHandlerSolution": {
        "SecretsManagerName": "RockMoss-SecretForUrlSignature",
        "SecretKey": "SecretForUrlSignature",
        "DistributionDomainName": "dy9thd0ewjmx0.cloudfront.net"
      }
    }
  },
  "ConnectionSettings": {
    "Postgres": {
      "Address": "<POSTGRES_ADDRESS>",
      "Port": "<POSTGRES_PORT>",
      "Username": "<POSTGRES_USERNAME>",
      "Password": "<POSTGRES_PASSWORD>",
      "Database": "<POSTGRES_DATABASE>"
